{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["standard", "react-app"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
    "import/no-anonymous-default-export": "off",
    "react-hooks/exhaustive-deps": "off"
  }
}
